.search_content {
  position: fixed;
  z-index: 9;
  background: #F8F8F8;
}
.search_content .search_wapper {
    background: #eaeaea;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: start;
    -webkit-justify-content: flex-start;
        -ms-flex-pack: start;
            justify-content: flex-start;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
    margin: 0 20rpx 20rpx;
    -webkit-border-radius: 5rpx;
            border-radius: 5rpx;
}
.search_content .search_wapper .search_img {
      width: 40rpx;
      height: 40rpx;
      margin: 0 10rpx;
}
.search_content .search_wapper .search_inp {
      width: 650rpx;
      height: 25px;
      padding: 10rpx 0;
}
.conten_list {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  padding: 0 20rpx;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
      -ms-flex-pack: start;
          justify-content: flex-start;
  -webkit-flex-wrap: wrap;
      -ms-flex-wrap: wrap;
          flex-wrap: wrap;
  padding-top: 130rpx;
}
.conten_list .search_imgs {
    padding: 0 18rpx;
    width: 200rpx;
    height: 260rpx;
    margin-top: 20rpx;
}
.conten_list .add_more {
    width: 100%;
    text-align: center;
}

